{
  "name": "parity",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "parity": "bin/parity.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean --dts",
    "dev": "tsup src/index.ts --watch --onSuccess \"node dist/index.js\"",
    "start": "node dist/index.js",
    "test": "vitest run",
    "lint": "tsc --noEmit",
    "check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.0.2",
    "@types/semver": "^7.7.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "execa": "^9.6.1",
    "semver": "^7.7.3",
    "yaml": "^2.8.2",
    "zod": "^4.2.1"
  }
}
